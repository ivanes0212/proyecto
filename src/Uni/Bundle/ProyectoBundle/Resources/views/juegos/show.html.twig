{% extends 'uniProyectoBundle::layout.html.twig' %}


{% block cuerpo %}
    <h1>juegos</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Titulojue</th>
                <td>{{ entity.tituloJue }}</td>
            </tr>
            <tr>
                <th>Descjue</th>
                <td>{{ entity.descJue }}</td>
            </tr>
            <tr>
                <th>Compjue</th>
                <td>{{ entity.compJue }}</td>
            </tr>
            <tr>
                <th>Fotojue</th>
                <td>{{ entity.fotoJue }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('juegos') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('juegos_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
